local CreditsFrame = Credits:addSection("Credits")
CreditsFrame:addButton("SHub made by Blue Duck#9891")
CreditsFrame:addButton("Copy Discord", function()
    if Clipboard ~= nil then
        Clipboard.set("Blue Duck#9891")
    elseif Synapse ~= nil then
        Synapse:Copy("Blue Duck#9891")
    elseif setclipboard ~= nil then
        setclipboard("Blue Duck#9891")
    end
end)
local UIDesigner = Credits:addSection("UI Designer")
UIDesigner:addButton("UI made by Blue Duck#9891")
local Scripter = Credits:addSection("Scripter")
Scripter:addButton("SHub was scripted by Blue Duck#9891")

local Settings = venyx:addPage("Settings", 5012544693)
local SettingsFrame = Settings:addSection("Settings")
SettingsFrame:addButton("Rejoin", function()
    venyx:Notify("Notifications", "Rejoining")
    wait(1)
    game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, game.JobId, game.Players.LocalPlayer)
end)
SettingsFrame:addButton("Switch Server", function()
    venyx:Notify("Switching Server")
    wait(1)
    game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
end)
SettingsFrame:addButton("Destroy UI", function()
    game.CoreGui.SHub:Destroy()
end)
SettingsFrame:addKeybind("Keybind", Enum.KeyCode.Equals, function()
    venyx:toggle()
end)

venyx:SelectPage(venyx.pages[1], true)
